  <!-- Odom -->
  <gazebo>
    <plugin name="odometry_plugin" filename="libodometry_plugin.so">
      <!-- Name of the link for which the odometry is required -->
      <frame>base</frame>
      <!-- Any arbitray name for the world frame -->
      <world_frame>fixed_world</world_frame>
    </plugin>
  </gazebo>

  <!-- IMU -->
  <gazebo reference="base">
  <gravity>true</gravity>
  <sensor name="imu_sensor" type="imu">
    <always_on>true</always_on>
    <update_rate>100</update_rate>
    <visualize>true</visualize>
    <topic>imu</topic>
    <plugin filename="libgazebo_ros_imu_sensor.so" name="imu_plugin">
      <topicName>imu</topicName>
      <bodyName>base</bodyName>
      <updateRateHZ>10.0</updateRateHZ>
      <gaussianNoise>0.0</gaussianNoise>
      <xyzOffset>0 0 0</xyzOffset>
      <rpyOffset>0 0 0</rpyOffset>
      <frameName>base</frameName>
      <initialOrientationAsReference>false</initialOrientationAsReference>
    </plugin>
    <pose>0 0 0 0 0 0</pose>
  </sensor>
  </gazebo> 

  <!-- # diff_drive -->
  <gazebo>
      <plugin name='skid_steer_drive' filename='libgazebo_ros_diff_drive.so'>
        <ros>
          <!-- Set namespace -->
          <namespace>/</namespace>
          <!-- Remap default topics -->
          <remapping>odom:=wheel_odom</remapping>
        </ros>
        <!-- Update rate -->
        <update_rate>500</update_rate>
        <!-- Number of wheel pairs -->
        <num_wheel_pairs>2</num_wheel_pairs>
        <!-- wheels0 -->
        <left_joint>rear_left_j</left_joint>
        <right_joint>rear_right_j</right_joint>
        <!-- wheels1-->
        <left_joint>front_left_j</left_joint>
        <right_joint>front_right_j</right_joint>
        <!-- kinematics -->
        <wheel_separation>0.8382</wheel_separation>
        <wheel_separation>0.8382</wheel_separation>
        <wheel_diameter>0.25224</wheel_diameter>
        <wheel_diameter>0.25224</wheel_diameter>
        <!-- limits -->
        <max_wheel_torque>20</max_wheel_torque>
        <max_wheel_acceleration>1.0</max_wheel_acceleration>
        <!-- output -->
        <publish_odom>true</publish_odom>
        <publish_odom_tf>true</publish_odom_tf>
        <publish_wheel_tf>true</publish_wheel_tf>
        <odometry_frame>odom</odometry_frame>
        <robot_base_frame>78</robot_base_frame>

      </plugin>
  </gazebo>

  <!-- Lidar -->
  <gazebo reference="walle_head">
  <sensor name="lidar" type="ray">
    <pose>0.0 0.0 0.0 -90 0 -90</pose>
    <always_on>true</always_on>
    <visualize>true</visualize>
    <update_rate>5</update_rate>
    <ray>
      <scan>
        <horizontal>
          <samples>360</samples>
          <resolution>1.00000</resolution>
          <min_angle>0.000000</min_angle>
          <max_angle>6.280000</max_angle>
        </horizontal>
      </scan>
      <range>
        <min>0.120000</min>
        <max>3.5</max>
        <resolution>0.015000</resolution>
      </range>
      <noise>
        <type>gaussian</type>
        <mean>0.0</mean>
        <stddev>0.01</stddev>
      </noise>
    </ray>
    <plugin name="scan" filename="libgazebo_ros_ray_sensor.so">
      <ros>
        <remapping>~/out:=scan</remapping>
      </ros>
      <output_type>sensor_msgs/LaserScan</output_type>
      <frame_name>base</frame_name>
    </plugin>
  </sensor>
  </gazebo>  

  <!-- Vaccum Gripper -->
  <gazebo>
  <plugin name='vacuum_gripper' filename='libgazebo_ros_vacuum_gripper.so'>

    <!-- This part can be commented out if not required -->
    <ros>

      <!-- Add a namespace -->

      <namespace>/</namespace>

      <!-- Remapping service and topic names -->
      <remapping>switch:=custom_switch</remapping>
      <remapping>grasping:=custom_grasping</remapping>
    </ros>

    <!-- Link associated with gripper -->
    <link_name>78</link_name>

    <!-- Max distance to attract entities -->
    <max_distance>1</max_distance>

    <!-- List of entities to be not attracted by the gripper -->
    <fixed>ground_plane</fixed>
    <fixed>wall</fixed>

  </plugin>
  </gazebo>


</robot>

